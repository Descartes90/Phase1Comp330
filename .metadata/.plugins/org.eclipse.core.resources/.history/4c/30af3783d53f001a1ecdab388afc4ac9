import com.google.gson.JsonElement;
import com.google.gson.JsonObject;
import com.google.gson.JsonParser;

public class Parser {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		JsonParser parser = new JsonParser();

		String json = "{\"How do you show equality in Java?\":\"Two equal signs\",\"What data types represent decimal numbers in Java?\":[\"Float\",\"Double\"],\"What is the % operator in Java?\":\"Gives the remainder\",\"Is Java an object-oriented language?\":true}";

		JsonElement jsonTree = parser.parse(json);

		if(jsonTree.isJsonObject()){
		    JsonObject jsonObject = jsonTree.getAsJsonObject();

		    JsonElement f1 = jsonObject.get("How do you show equality in Java?");

		    JsonElement f2 = jsonObject.get("What data types represent decimal numbers in Java?");
		    
		    JsonElement f3 = jsonObject.get("What is the % operator in Java?");
		    
		    JsonElement f4 = jsonObject.get("Is Java an object-oriented language?");
		    
		    System.out.println();
		    }
		
		}
	}
